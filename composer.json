{
    "name": "pkboom/laravel-route-usage",
    "description": "",
    "keywords": [
        "pkboom",
        "laravel-route-usage"
    ],
    "homepage": "https://github.com/pkboom/laravel-route-usage",
    "license": "MIT",
    "authors": [
        {
            "name": "Keunbae Park",
            "email": "ppotpo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "orchestra/testbench": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\RouteUsage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkboom\\RouteUsage\\RouteUsageServiceProvider"
            ]
        }
    }
}
