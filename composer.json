{
  "name": "pkboom/laravel-route-usage-counter",
  "description": "",
  "keywords": [
    "pkboom",
    "laravel-route-usage-counter"
  ],
  "homepage": "https://github.com/pkboom/laravel-route-usage-counter",
  "license": "MIT",
  "authors": [
    {
      "name": "Keunbae Park",
      "email": "ppotpo@gmail.com"
    }
  ],
  "require": {
    "symfony/var-dumper": "^4.2",
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Pkboom\\RouteUsageCounter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pkboom\\RouteUsageCounter\\RouteUsageCounterServiceProvider"
      ],
      "aliases": {
        "RouteUsageCounter": "Pkboom\\RouteUsageCounter\\Facade\\RouteUsageCounter"
      }
    }
  }
}
